<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".SellingSDECourseActivity"
    tools:showIn="@layout/activity_selling_sdecourse">

    <TextView
        android:id="@+id/browseCourseS"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="@font/aldrich"
        android:text="Courses"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        android:textColor="@color/colorPrimaryDark" />

    <Button
        android:id="@+id/archiS"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Architecture" />

    <Button
        android:id="@+id/indsysS"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Industrial Systems" />

    <Button
        android:id="@+id/pfmS"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Project Facilities N management" />

    <Button
        android:id="@+id/reS"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Real Estate" />


</LinearLayout>